{
  "name": "pumpfun-twitter-launcher",
  "version": "1.0.0",
  "description": "Twitter-triggered PumpFun token launcher via PumpPortal with Web UI",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node start.ts",
    "launcher": "ts-node start.ts",
    "cli": "ts-node src/cli.ts",
    "bot": "ts-node src/index.ts",
    "build": "tsc",
    "build:web": "cd web && npm install && npm run build",
    "build:all": "npm run build && npm run build:web",
    "start:prod": "node dist/index.js",
    "cli:prod": "node dist/cli.js",
    "dev": "ts-node src/index.ts",
    "create:wizard": "ts-node src/trade-wizard.ts",
    "create:tui": "ts-node src/trade-tui.ts",
    "create:trade": "ts-node src/trade-create.ts",
    "create:enhanced": "ts-node src/trade-create-enhanced.ts",
    "create:local": "ts-node src/trade-local-create.ts",
    "dev:web": "cd web && npm run dev",
    "desktop:dev": "concurrently \"cd web && npm run dev\" \"wait-on http://localhost:5173 && electron desktop/main.js dev\"",
    "desktop": "npm run build:web && electron desktop/main.js",
    "mcp": "ts-node src/mcp-server.ts",
    "db:push": "node scripts/apply-supabase-migrations.js",
    "watch": "tsc -w",
    "setup": "ts-node start.ts",
    "wallet:new": "ts-node src/generate-wallet.ts",
    "wallet:vanity": "ts-node src/vanity-wallet.ts",
    "desktop:pack": "npm run build:web && electron-builder"
  },
  "keywords": [
    "pumpfun",
    "solana",
    "twitter",
    "token-launcher"
  ],
  "license": "MIT",
  "dependencies": {
    "@cryptoscan/pumpfun-sdk": "^1.3.1",
    "@modelcontextprotocol/sdk": "^1.22.0",
    "@pump-fun/pump-sdk": "^1.22.1",
    "@raydium-io/raydium-sdk-v2": "^0.2.30-alpha",
    "@solana/spl-token": "^0.4.14",
    "@solana/web3.js": "^1.87.6",
    "@supabase/supabase-js": "^2.84.0",
    "@types/multer": "^2.0.0",
    "@types/prompts": "^2.4.9",
    "bs58": "^5.0.0",
    "chalk": "^4.1.2",
    "concurrently": "^8.2.2",
    "cookie-parser": "^1.4.7",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "eventemitter3": "^5.0.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "jito-ts": "^4.2.1",
    "multer": "^2.0.2",
    "node-fetch": "^2.7.0",
    "node-telegram-bot-api": "^0.64.0",
    "ora": "^5.4.1",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "prompts": "^2.4.2",
    "pumpdotfun-sdk": "^1.4.2",
    "snarkjs": "^0.7.5",
    "twitter-api-v2": "^1.15.2",
    "undici": "^6.19.4",
    "uuid": "^9.0.1",
    "wait-on": "^7.2.0",
    "ws": "^8.18.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/node-fetch": "^2.6.9",
    "@types/node-telegram-bot-api": "^0.64.2",
    "@types/uuid": "^9.0.7",
    "electron": "^39.2.3",
    "electron-builder": "^24.13.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.2"
  },
  "build": {
    "appId": "com.pumplauncher.app",
    "productName": "PumpLauncher",
    "files": [
      "dist/**/*",
      "desktop/**/*",
      "web/dist/**/*",
      "package.json"
    ],
    "extraMetadata": {
      "main": "desktop/main.js"
    },
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "target": [
        "dmg"
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ]
    }
  }
}
