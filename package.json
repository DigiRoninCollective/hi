{
  "name": "pumpfun-twitter-launcher",
  "version": "1.0.0",
  "description": "Twitter-triggered PumpFun token launcher via PumpPortal with Web UI",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:web": "cd web && npm install && npm run build",
    "build:all": "npm run build && npm run build:web",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "dev:web": "cd web && npm run dev",
    "watch": "tsc -w"
  },
  "keywords": [
    "pumpfun",
    "solana",
    "twitter",
    "token-launcher"
  ],
  "license": "MIT",
  "dependencies": {
    "@solana/web3.js": "^1.87.6",
    "@supabase/supabase-js": "^2.84.0",
    "bs58": "^5.0.0",
    "cookie-parser": "^1.4.7",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "eventemitter3": "^5.0.1",
    "express": "^4.18.2",
    "node-fetch": "^2.7.0",
    "node-telegram-bot-api": "^0.64.0",
    "snoowrap": "^1.23.0",
    "twitter-api-v2": "^1.15.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/node-fetch": "^2.6.9",
    "@types/node-telegram-bot-api": "^0.64.2",
    "@types/uuid": "^9.0.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.2"
  }
}
